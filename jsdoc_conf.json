{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": [
            "storage/index.js",
            "auth/index.js",
            "messaging/index.js",
            "README_docs.md"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "schmuckliCloud JS SDK",
        "disableSort": true,
        "collapse": false,
        "resources": {
            "Docs": "https://docs.schmuckli.cloud/"
        }
    },
    "opts": {
        "destination": "./out/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./jsdoc-template"
    }
}